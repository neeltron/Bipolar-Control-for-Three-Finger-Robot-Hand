    <light pos="2 0 2" dir="-1 0 -1" directional="true" diffuse="0.3 0.3 0.3" castshadow="false"/>
    <light pos="-2 0 2" dir="1 0 -1" directional="true" diffuse="0.3 0.3 0.3" castshadow="false"/>
