<mujoco model="right_shadow_hand scene">
  <include file="pos_mid_tip_pad_right_handv3.xml"/>

  <option solver="CG" tolerance="1e-6" timestep=".001" integrator="implicitfast"/>

  <size memory="10M"/>

  <compiler meshdir="assets"/>

  <statistic extent="0.3" center="0.3 0 0"/>

  <!--visual>
    <rgba haze="0.15 0.25 0.35 1"/>
    <quality shadowsize="8192"/>
    <global azimuth="220" elevation="-30"/>
  </visual-->

<visual>
  <!-- Softer haze and lighting to avoid distracting background -->
  <!--rgba haze="0.1 0.08 0.08 1"/-->
  <rgba haze="0 0 0 0"/>
  <quality shadowsize="8192"/>
  <global azimuth="220" elevation="-30"/>
</visual>




  <asset>
    <!-- Sky and ground textures -->
    <!--texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge"
             rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
             markrgb="0.8 0.8 0.8" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/-->

    <!-- Simple neutral background + plain floor -->
    <!--texture type="skybox" builtin="gradient" rgb1="0.95 0.88 0.88" rgb2="0.85 0.75 0.75" width="512" height="3072"/>
    <material name="floor_mat" rgba="0.95 0.88 0.88 1" specular="0" shininess="0"/-->

    <texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2="1 1 1" width="512" height="3072"/>
    <material name="floor_mat" rgba="1 1 1 1" specular="0" shininess="0"/>


    <!-- Triangular prism mesh -->
    <!--mesh name="tri_prism" file="triangular_prism.stl" scale="1 1 1"/-->

    <!-- Realistic object mesh stapler-->
    <!--mesh name="My_Scan_9" file="My_Scan_9.stl" scale="0.0007 0.0007 0.0007"/-->


    <!-- Realistic object mesh mouse -->
    <!--mesh name="Mouse" file="Mouse.stl" scale="0.00143 0.00143 0.00143"/-->

        <!-- Letter textures (files in assets/letters/) -->
    <texture type="2d" name="tex_A" file="assets/letters/A.png"/>
    <texture type="2d" name="tex_B" file="assets/letters/B.png"/>
    <texture type="2d" name="tex_C" file="assets/letters/C.png"/>
    <texture type="2d" name="tex_D" file="assets/letters/D.png"/>
    <texture type="2d" name="tex_E" file="assets/letters/E.png"/>
    <texture type="2d" name="tex_F" file="assets/letters/F.png"/>

    <!-- Materials that apply those textures -->
    <material name="face_A" texture="tex_A" texuniform="false" texrepeat="1 1"/>
    <material name="face_B" texture="tex_B" texuniform="false" texrepeat="1 1"/>
    <material name="face_C" texture="tex_C" texuniform="false" texrepeat="1 1"/>
    <material name="face_D" texture="tex_D" texuniform="false" texrepeat="1 1"/>
    <material name="face_E" texture="tex_E" texuniform="false" texrepeat="1 1"/>
    <material name="face_F" texture="tex_F" texuniform="false" texrepeat="1 1"/>



    <!-- Bumpy cylinder mesh and material -->
    <mesh name="bumpy_cylinder" file="bumpy_cylinder.stl" scale="1 1 1"/>
    <material name="bumpy_material" rgba="0.8 0.4 0.2 1" reflectance="0.1"/>
    <!--new additions-->
    <material name="face_red" rgba="1 0 0 1"/>
    <material name="face_green" rgba="0 1 0 1"/>
    <material name="face_blue" rgba="0 0 1 1"/>
    <material name="face_yellow" rgba="0.6 0 0.8 1"/>  <!--rgba="1 1 0 1-->
    <material name="face_purple" rgba="0.6 0 0.8 1"/>
    <material name="face_white" rgba="1 1 1 1"/>   <!--rgba="1 1 1 1-->

        <!-- Red/Yellow split for the cylinder side (two halves) -->
    <texture type="2d" name="red_yellow_half_tex" builtin="checker"
             rgb1="1 0 0" rgb2="1 1 0" width="2048" height="1"/>
    <material name="red_yellow_half_mat" texture="red_yellow_half_tex"
             texuniform="false" texrepeat="1 1" />
  </asset>

  <worldbody>
    <!-- Lights and floor -->
    <!--light pos="0 0 1"/>
    <light pos="0.3 0 1.5" dir="0 0 -1" directional="true"/>
    <geom name="floor" pos="0 0 -0.1" size="0 0 0.01" type="plane" material="groundplane"/-->

